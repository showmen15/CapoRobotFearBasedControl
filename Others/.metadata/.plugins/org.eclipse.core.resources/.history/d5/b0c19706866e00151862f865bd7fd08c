package pl.edu.agh.capo.fear.data;

import java.io.Serializable;

public class LocationInTime implements Serializable {

	private static final long serialVersionUID = -524827480308179007L;
	protected Location location;
	protected double time;
	
	public LocationInTime(Location location, double time) {
		super();
		this.location = location;
		this.time = time;
	}

	public Location getLocation() {
		return location;
	}

	public double getTime() {
		return time;
	}
	
	
	
	
}
